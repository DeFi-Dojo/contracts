{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/prefer-default-export": "warn",
    "no-console": "off",
    "@typescript-eslint/quotes": ["warn", "double"]
  }
}
